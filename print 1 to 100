for number in range(1, 101):
    output = ""
    if number % 3 == 0 and number % 5:
        output += "Divided by both"
    elif number % 3 == 0:
        output += "Divided by 3"
    elif number % 5 == 0:
        output += "divided by 5"
    elif output == "":
        output = number
    print(output)
